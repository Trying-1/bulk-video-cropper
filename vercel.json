{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://bulkvidcropper.com",
    "NEXT_PUBLIC_APP_ENV": "production",
    "NEXT_PUBLIC_MAX_FILE_SIZE": "100"
  }
}
